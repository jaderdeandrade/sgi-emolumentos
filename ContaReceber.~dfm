  object sqlreceber: TSQLDataSet
    CommandText = 
      'select * from CONTASRECEBER where (CONTASRECEBER .ATIVO='#39'1'#39') and' +
      ' (CONTASRECEBER. CLIENTE_IDCLIENTE=:idcliente )'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
